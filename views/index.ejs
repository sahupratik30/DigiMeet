  <body>
    <!-- Navbar section start -->
    <nav class="navbar">
      <!-- Navbar Logo Section -->
      <div class="navbar__logo">
        <img src="/images/logo.png" alt="logo" class="logo" />
      </div>
      <!-- Navbar Links Section -->
      <div class="navbar__links">
        <a href="">join a meeting</a>
        <a href="/room" id="hostBtn">host a meeting</a>
      </div>
    </nav>
    <!-- Navbar Section End -->
    <!-- Main Section Start -->
    <section class="main__section">
      <!-- Center section start -->
      <div class="main__section__center">
        <h2>Join Meeting</h2>
        <div class="center__input">
          <label for="meeting__id">Meeting ID</label>
          <input
            type="text"
            id="meeting__id"
            placeholder="Enter Meeting ID"
            required
          />
        </div>
        <p>
          By clicking "Join", you agree to our
          <span>Terms of Services</span> and <span>Privacy Statement</span>
        </p>
        <div class="center__button">
          <button type="submit" id="submit__button" disabled>Join</button>
        </div>
      </div>
    </section>
    <!-- Main Section End -->

